<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:foo="http://schemas.android.com/apk/res/com.assignmentexpert"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#000000" 
    android:focusableInTouchMode="true"
    android:focusable="true">
  <TableLayout 
      xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:foo="http://schemas.android.com/apk/res/com.assignmentexpert"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:shrinkColumns="0"
   >
   <TableRow
          android:id="@+id/tableRow1"
          android:layout_width="fill_parent"
          android:gravity="center"
          >
      <com.customitems.CustomTextView
   	    
        android:id="@+id/textViewPlus1"
        android:layout_width="fill_parent"
        style="@style/custom_textview"
        
        android:text="Sign up completed. Please add a phone number."
        foo:customFont="Roboto-Medium.ttf">
    </com.customitems.CustomTextView>
     </TableRow>
          <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
    <TableRow
          android:id="@+id/tableRow1"
          android:layout_width="fill_parent"
          >
        <com.customitems.CustomEditPreference
            android:id="@+id/registerName"
            android:layout_marginLeft="0dp"
            android:title="Name"
            android:layout_marginRight="0dp"
            foo:iconDisable="1"
          	android:layout_width="fill_parent"
            android:layout_height="64dp"
            android:layout_weight="1"
            />
            
          </TableRow>
          <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		 <TableRow
          android:id="@+id/tableRow1"
          android:layout_width="fill_parent"
          >
        <com.customitems.CustomEditPreference
            android:id="@+id/registerEmail"
            android:layout_marginLeft="0dp"
            foo:iconDisable="1"
            android:title="Email"
            android:layout_marginRight="0dp"
          	android:layout_width="fill_parent"
            android:layout_height="64dp"
            android:layout_weight="1"
            />
            
          </TableRow>
          <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
    <TableRow
          android:id="@+id/tableRow1"
          android:layout_width="fill_parent"
          >
        <com.customitems.CustomEditPreference
            android:id="@+id/counrtyEditPref"
            android:layout_marginLeft="0dp"
            android:title="Country"
            android:layout_marginRight="0dp"
          	android:layout_width="fill_parent"
            android:layout_height="64dp"
            android:layout_weight="1"
            />
            
          </TableRow>
          <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
      <TableRow
          android:id="@+id/tableRow1"
          style="@style/text_row" 
          android:layout_width="fill_parent"
          android:layout_marginRight="0dp"
          android:layout_marginLeft="0dp"
          android:layout_weight="1"
          >
  <com.customitems.CustomEditText
        android:id="@+id/registerPhone"
        style="@style/custom_edittext"
        android:inputType = "number"
        android:layout_marginRight="10dp"
        android:layout_width="fill_parent"
        foo:customFont="Roboto-Regular.ttf"
        android:hint="Phone"/>
  	<ImageView
     android:id="@+id/orderInfo"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="0dp"
    style="@style/custom_button"
     android:src="@drawable/info_big"
      android:background="@drawable/info_button"
   />
  </TableRow>
		 <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		 <TableRow
          android:id="@+id/tableRow1"
          style="@style/text_row" 
           android:layout_marginRight="0dp"
          android:layout_marginLeft="0dp"
          android:layout_weight="1"
          >
	 
    <com.customitems.CustomTextView
               android:id="@+android:id/title"
     		   android:layout_width="wrap_content"
     		   style="@style/custom_edittext"
     		  android:text="Sign me in when AE starts"
              foo:customFont="Roboto-Medium.ttf"/>

 	<CheckBox
   	   android:id="@+id/signMeCheck"
        android:layout_width="wrap_content" 
        android:layout_height="wrap_content" 
        android:text=""
        android:gravity="left"
        android:checked="true"
        android:textSize="20sp"/>

  </TableRow>
   <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
   <TableRow
          android:id="@+id/tableRow3"
          style="@style/button_row" 
          >
 	    <Button
            android:id="@+id/btnProceed"
            style="@style/custom_button"
            android:layout_marginRight="0dp"
            android:layout_marginLeft="0dp"
            android:text="Finish"
             />
	    </TableRow>
		 
  </TableLayout>

  </ScrollView>