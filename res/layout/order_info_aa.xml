<?xml version="1.0" encoding="UTF-8"?>
     <ScrollView
		    android:id="@+id/scrollOrderInfo"
		    xmlns:android="http://schemas.android.com/apk/res/android"
		    xmlns:foo="http://schemas.android.com/apk/res/com.assignmentexpert"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:background="#000000"
		    >
	     <RelativeLayout
		    android:id="@+id/panelInteractions"
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:background="#000000"
		    android:orientation="horizontal" >
  
	     
	         <LinearLayout
	        android:id="@+id/productPriceLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="60dp"
		    android:baselineAligned="false"
		    android:orientation="horizontal"
		    android:weightSum="1.0" >
		      <LinearLayout
	        android:id="@+id/productLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >

	     <com.customitems.CustomTextView
	         android:id="@+id/productLabel"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:layout_width="fill_parent"
	         android:gravity="left|bottom"
	         android:paddingLeft="10dip"
	         android:paddingTop="10dip"
	         android:text="Product:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     <com.customitems.CustomTextView
	         android:id="@+id/productTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="top"
	         foo:customFont="Roboto-Medium.ttf"
			 android:paddingLeft="10dp"	
			 android:text=""        
	         android:textColor="#FFFFFF"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     </LinearLayout>
	       <LinearLayout
	        android:id="@+id/citationLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >
	      <com.customitems.CustomTextView
	         android:id="@+id/priceLabel"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="right"
	         foo:customFont="Roboto-Medium.ttf"
	         android:paddingRight="10dip"
	         android:paddingTop="10dip"
	         android:text="Price:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         
	         />
	       <com.customitems.CustomTextView
	         android:id="@+id/priceTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="right|top"
	         foo:customFont="Roboto-Medium.ttf"
	         android:paddingRight="10dp"	
	         android:textColor="#FFFFFF"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         />
	       </LinearLayout>
	       
	       
	</LinearLayout>
	 <LinearLayout
	        android:id="@+id/subjTimezoneLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="60dp"
		    android:layout_below="@+id/productPriceLayout"
		    android:baselineAligned="false"
		    android:orientation="horizontal"
		    android:weightSum="1.0" >
		      <LinearLayout
	        android:id="@+id/subjectLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >

	     <com.customitems.CustomTextView
	         android:id="@+id/subjLabel"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:layout_width="fill_parent"
	         android:gravity="left|bottom"
	         android:paddingLeft="10dip"
	         android:paddingTop="10dip"
	         android:text="Subject:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     <com.customitems.CustomTextView
	         android:id="@+id/subjTextView"
	         android:layout_width="fill_parent"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:gravity="left|top"
	         android:paddingLeft="10dp"
	         android:text=""
	         android:textColor="#FFFFFF"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     </LinearLayout>
	       <LinearLayout
	        android:id="@+id/timezoneLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >
	      <com.customitems.CustomTextView
	         android:id="@+id/timezoneLabel"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         foo:customFont="Roboto-Medium.ttf"
	       android:gravity="right"
	         android:paddingRight="10dip"
	         android:paddingTop="10dip"
	         android:text="Timezone:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         
	         />
	       <com.customitems.CustomTextView
	         android:id="@+id/timezoneTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="right|top"
	          android:paddingRight="10dp"	
	         android:text=""
	         foo:customFont="Roboto-Medium.ttf"
	         android:textColor="#FFFFFF"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         />
	       </LinearLayout>
	       
	       
	</LinearLayout>
	 <LinearLayout
	        android:id="@+id/categoryPostedLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="60dp"
		    android:layout_below="@+id/subjTimezoneLayout"
		    android:baselineAligned="false"
		    android:orientation="horizontal"
		    android:weightSum="1.0" >
		      <LinearLayout
	        android:id="@+id/categoryLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >

	     <com.customitems.CustomTextView
	         android:id="@+id/CategoryLabel"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:layout_width="fill_parent"
	         android:gravity="left|bottom"
	         android:paddingLeft="10dip"
	         android:paddingTop="10dip"
	         android:text="Category:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     <com.customitems.CustomTextView
	         android:id="@+id/categoryTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         foo:customFont="Roboto-Medium.ttf"
	         android:gravity="left|top"
	          android:paddingLeft="10dp"
	         android:text=""
	         android:textColor="#FFFFFF"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     </LinearLayout>
	       <LinearLayout
	        android:id="@+id/timezoneLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >
	      <com.customitems.CustomTextView
	         android:id="@+id/postedLabel"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         foo:customFont="Roboto-Medium.ttf"
	        android:gravity="right"
	         android:paddingRight="10dip"
	         android:paddingTop="10dip"
	         android:text="Posted:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         
	         />
	       <com.customitems.CustomTextView
	         android:id="@+id/postedTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="right|top"
	          android:paddingRight="10dp"	
	          foo:customFont="Roboto-Medium.ttf"
	         android:text=""
	         android:textColor="#FFFFFF"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         />
	       </LinearLayout>
	       
	       
	</LinearLayout>
	<LinearLayout
	        android:id="@+id/levelDeadlineLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="60dp"
		    android:layout_below="@+id/categoryPostedLayout"
		    android:baselineAligned="false"
		    android:orientation="horizontal"
		    android:weightSum="1.0" >
		      <LinearLayout
	        android:id="@+id/levelLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >

	     <com.customitems.CustomTextView
	         android:id="@+id/levelLabel"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:layout_width="fill_parent"
	          android:gravity="left|bottom"
	         android:paddingLeft="10dip"
	         android:paddingTop="10dip"
	         android:text="Level:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     <com.customitems.CustomTextView
	         android:id="@+id/levelTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="left|top"
	         foo:customFont="Roboto-Medium.ttf"
	         android:text=""
	          android:paddingLeft="10dp"
	         android:textColor="#FFFFFF"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	     </LinearLayout>
	       <LinearLayout
	        android:id="@+id/deadlineLayout"
		    android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:orientation="vertical"
		    android:layout_weight=".50"
		     >
	      <com.customitems.CustomTextView
	         android:id="@+id/deadlineLabel"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         foo:customFont="Roboto-Medium.ttf"
	     	 android:gravity="right"
	         android:paddingRight="10dip"
	         android:paddingTop="10dip"
	         android:text="Deadline:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         
	         />
	       <com.customitems.CustomTextView
	         android:id="@+id/deadlineTextView"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:gravity="right|top"
	         android:paddingRight="10dp"	
	         foo:customFont="Roboto-Medium.ttf"
	         android:text=""
	         android:textColor="#FFFFFF"
	         android:textStyle="bold" 
	         android:textSize="14dp"
	         />
	       </LinearLayout>
	       
	       
	</LinearLayout>
	 <com.customitems.CustomTextView
	         android:id="@+id/requireLabel"
	         android:layout_width="fill_parent"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:gravity="left|bottom"
	         android:paddingLeft="10dip"
	         android:paddingTop="10dip"
	         android:layout_below="@+id/levelDeadlineLayout"
	         android:text="Requirements:"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	 <com.customitems.CustomTextView
	         android:id="@+id/requireTextView"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_width="fill_parent"
	         android:layout_height="60dp"
	         android:gravity="left|top"
	         android:text=""
	          android:paddingLeft="10dp"
	         android:layout_below="@+id/requireLabel"
	         android:textColor="#FFFFFF"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	         
	         <com.customitems.CustomTextView
	         android:id="@+id/tasklabel"
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         foo:customFont="Roboto-Medium.ttf"
	         android:gravity="left|top"
	         android:layout_below="@+id/requireTextView"
	         android:text="Task:"
	          android:paddingLeft="10dip"
	         android:textColor="#a5a5a5"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	 <com.customitems.CustomTextView
	         android:id="@+id/taskTextView"
	         android:layout_width="fill_parent"
	         foo:customFont="Roboto-Medium.ttf"
	         android:layout_height="wrap_content"
	         android:gravity="left|top"
	         android:text=""
	         
	         android:paddingLeft="10dp"
	         android:layout_below="@+id/tasklabel"
	         android:textColor="#FFFFFF"
	         android:textStyle="bold"
	         android:textSize="14dp" />
	 <HorizontalScrollView 
	        android:layout_width="fill_parent"
		    android:layout_height="75dp"
		    android:layout_below="@+id/taskTextView"
		    android:background="#000000"
		     >	   
		     
	        <LinearLayout
	        android:id="@+id/infoMessageListLayout"
		    android:layout_width="wrap_content"
		    android:layout_height="75dp"
		    android:orientation="horizontal">
		    </LinearLayout>
		    
		    </HorizontalScrollView>
	     </RelativeLayout>
	    </ScrollView>
	    
	    
	    

