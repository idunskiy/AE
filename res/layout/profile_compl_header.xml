<?xml version="1.0" encoding="utf-8"?>
 <TableLayout 
      xmlns:android="http://schemas.android.com/apk/res/android"
     xmlns:foo="http://schemas.android.com/apk/res/com.assignmentexpert"
    android:id="@+id/tableLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:stretchColumns="0"
    android:shrinkColumns="2"
    >
          <TableRow
          android:id="@+id/tableRow1"
          style="@style/text_row" 
          android:layout_weight="1">
   	<com.customitems.CustomTextView
        android:id="@+id/textViewPlus1"
        android:layout_width="match_parent"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        style="@style/custom_textview"
        android:text="@string/profileText"
        foo:customFont="Roboto-Medium.ttf">
    </com.customitems.CustomTextView>
		 </TableRow>
		 <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		  <TableRow
        android:id="@+id/tableRow2"
         style="@style/edit_row"
         android:layout_width="fill_parent"
       >
   	<com.customitems.CustomEditText
        android:id="@+id/registerName"
        style="@style/custom_edittext"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_width="fill_parent"
        foo:customFont="Roboto-Regular.ttf"
        android:hint="First name">
    </com.customitems.CustomEditText>
         </TableRow>
         	 <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		  <TableRow
        android:id="@+id/tableRow1"
         style="@style/edit_row"
         android:layout_width="fill_parent"
        >
   	<com.customitems.CustomEditText
        android:id="@+id/registerEmail"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="25dp"
        android:layout_width="fill_parent"
        style="@style/custom_edittext"
        foo:customFont="Roboto-Regular.ttf"
        android:hint="Phone number"/>
    <ImageView
     android:id="@+id/phoneInfo"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="0dp"
    android:maxWidth="10dp"
    style="@style/custom_button"
     android:src="@drawable/info_big"
      android:background="@drawable/info_button"
   />
	    </TableRow>
	 <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		  <TableRow
        android:id="@+id/tableRow1"
        android:layout_width="fill_parent"
         style="@style/edit_row"
        >
   	<com.customitems.CustomEditText
        android:id="@+id/registerPassword"
        style="@style/custom_edittext"
        foo:customFont="Roboto-Regular.ttf"
        android:layout_width="fill_parent"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:hint="Password">
    </com.customitems.CustomEditText>
	    </TableRow>
	 <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		  <TableRow
        android:id="@+id/tableRow1"
         style="@style/edit_row"
         android:layout_width="fill_parent"
        >
   	<com.customitems.CustomEditText
        android:id="@+id/registerPasswordconf"
        style="@style/custom_edittext"
        android:layout_width="fill_parent"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        foo:customFont="Roboto-Regular.ttf"
        android:hint="Password confirmation">
    </com.customitems.CustomEditText>
	    </TableRow>
	 <TableRow android:layout_height="1px" android:background="#323232">
   	<TextView android:layout_span="1" android:layout_height="1px" 
             android:layout_width="fill_parent" android:text="">
  	 </TextView>
		</TableRow>
		
		
		 
	</TableLayout>
